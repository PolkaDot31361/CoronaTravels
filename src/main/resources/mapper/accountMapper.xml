<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="scit.classa.three.dao.AccountMapper">
	<insert id="createAccount" parameterType="account">
	
		insert into
		  Travel_user(
		      user_id     
			,user_pwd    
			,user_name   
			,user_email  
			,user_gender      
			,user_nickname 
		)values(
		    #{user_id}   
			,#{user_pwd}    
			,#{user_name}   
			,#{user_email}
			,#{user_gender}  
			,#{user_nickname}
		)
	
	</insert>

</mapper>
